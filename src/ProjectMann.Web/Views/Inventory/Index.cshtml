@model IEnumerable<ProjectMann.Core.Domain.ItemInventario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreSoftware)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Licencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Version)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaCreacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaModificacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkAsignadoANavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkUsuarioCreaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkUsuarioModificaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreSoftware)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Licencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Version)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaCreacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaModificacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkAsignadoANavigation.Apellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkUsuarioCreaNavigation.Apellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkUsuarioModificaNavigation.Apellido)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdItemInventario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdItemInventario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdItemInventario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
